@model AppUser

@{
    ViewData["Title"] = "Profil";
}

<h2>Hoş geldin, @Model.AdSoyad!</h2>

<ul class="list-group mt-3">
    <li class="list-group-item"><strong>Email:</strong> @Model.EmailAdres</li>
    <li class="list-group-item"><strong>Rol:</strong> @Model.Rol</li>
</ul>

<p class="mt-3"><a class="btn btn-outline-secondary" href="@Url.Action("Giris", "Kullanici")">Çıkış Yap</a></p>

